  <div class="swiper-wrapper">
    {% for post in collections.post %}
      <div class="swiper-slide">
        <div class="introducde" style="background-image: url({{ post.data.image }});">
          <div class="introduce-content">
            <h1 class="animate__animated animate__backInDown">{{ post.data.title }}</h1>
            <p class="animate__animated animate__backInDown">{{ post.data.author }}</p>
            <p style="width:60%; text-align:center;margin: 0 auto">{{ post.data.description }}</p>
            <button>Read more</button>
          </div>
        </div>
      </div>
    {% endfor %}
  </div>
</section>


